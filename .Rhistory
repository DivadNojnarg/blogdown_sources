nicholson <- function(t, y, a, lambda, c) {
X <- y[1]
Y <- y[2]
f    <- exp(-a * Y)
Xnew <- lambda * X * f
Ynew <- c * X * (1- f)
list (c(Xnew, Ynew))
}
