RobertsonDAE <- function(t, y, dy, parms) {
res1 <- -dy[1] - 0.04*y[1] + 1e4*y[2]*y[3]
res2 <- -dy[2] + 0.04*y[1] - 1e4*y[2]*y[3] - 3e7* y[2]^2
res3 <- - 1 + y[1] + y[2] + y[3]
list(c(res1, res2, res3))
}
